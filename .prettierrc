{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 140,
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["cn", "classNames", "clsx", "twMerge", "twJoin", "tm", "tj", "tv"],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "<BUILTIN_MODULES>",
    "^react(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^~/(.*)$",
    "^@/(.*)$",
    "^[.]",
    "",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
